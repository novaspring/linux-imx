			compatible = "arm,cortex-a35";
			reg = <0x00 0x00>;
			enable-method = "psci";
			next-level-cache = <0x02>;
			clocks = <0x03 0x1fb 0x02>;
			operating-points-v2 = <0x04>;
			#cooling-cells = <0x02>;
